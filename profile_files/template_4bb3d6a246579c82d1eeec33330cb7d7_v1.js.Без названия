let searchBtn = document.querySelectorAll('.js-search');
let burgerMenu = document.querySelector('.burger-menu');
let customHeader = document.querySelector('.js-header').classList;

let active_class = "header-sticky";

let logoImage = document.querySelector('.logo__image');
let headerBurger = document.getElementById('headerBurger') || false;
let headerClose = document.getElementById('headerClose') || false;
let burgerLine = document.getElementById('burgerLine') || false;
let headerSideContent = document.getElementById('headerSideContent') || false;
let headerSide = document.getElementById('headerSide') || false;

let catalog = document.getElementById('catalog') || false;
let catalogchange = document.getElementById('catalog-menu') || false;
let catalogback = document.getElementById('catalog-back') || false;
let subcatalog = document.getElementById('sub-catalog') || false;

let mobHeader = document.querySelector(".header-sticky");

if (mobHeader.classList.contains('header-sticky')) {
  mobHeader.classList.remove("header-sticky");
}

window.addEventListener('scroll', function (e) {

  if (pageYOffset > 600) {
    customHeader.add(active_class);
    logoImage.src = '/local/templates/main/img/logo-min.svg';

    //console.log("add");
    mobHeader.classList.add("header-sticky");


  } else {
    customHeader.remove(active_class);
    logoImage.src = '/local/templates/main/img/logo-new.svg';

    //console.log("remove");
    mobHeader.classList.remove("header-sticky");

  }

  // document.querySelector(':root').style.setProperty('--header-height', `${headerHeight}px`);
});

//      
if (!!searchBtn)
  for (searchBtnItem of searchBtn) {
    searchBtnItem.onclick = function (r) {

      if (window.innerWidth >= 680) {
        r.preventDefault();
        document.body.classList.toggle('search-show');
        document.getElementById('headerSideContent').classList.remove('show');
        document.getElementById('headerSide').classList.remove('active');
      } else {
        // document.body.classList.toggle('search-show-mob');
        // document.getElementById('headerSideContent2').classList.remove('show');
        // document.getElementById('headerSide2').classList.remove('active');
      }


    }

  };




//       
if (catalogchange) {
  catalogchange.addEventListener("click", function (event) {
    catalog.classList.add("hidden");
    subcatalog.classList.remove("hidden");
  })
};
if (catalogback) {
  catalogback.addEventListener("click", function (event) {
    catalog.classList.remove("hidden");
    subcatalog.classList.add("hidden");
  })
};
//         ----------------------

//      --------------------------------------------
if (headerBurger) {
  headerBurger.addEventListener("click", function (event) {
    headerSide.classList.toggle('active');
    headerBurger.classList.toggle('active');
    burgerLine.classList.toggle('active');
    headerSideContent.classList.toggle('show');
    document.body.classList.add('lock');
  });

  headerClose.addEventListener("click", function (event) {
    headerSide.classList.toggle('active');
    headerBurger.classList.toggle('active');
    burgerLine.classList.toggle('active');
    headerSideContent.classList.toggle('show');
    document.body.classList.remove('lock');
  })

}

//       --------------------------------------------

Inputmask("+7 (999) 999-99-99").mask('[type="tel"]');



